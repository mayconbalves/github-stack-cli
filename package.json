{
  "name": "github-stack-cli",
  "version": "1.1.0",
  "description": "CLI for analyzing users and repositories on GitHub",
  "module": "index.ts",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "github-stack": "./dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "dev": "bun index.ts",
    "start": "bun index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "prepare": "bun run format && bun run lint && bun test",
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "github",
    "cli",
    "stack",
    "analysis",
    "developer-tools",
    "bun"
  ],
  "author": "github-stack-cli contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayconbalves/github-stack-cli.git"
  },
  "bugs": {
    "url": "https://github.com/mayconbalves/github-stack-cli/issues"
  },
  "homepage": "https://github.com/mayconbalves/github-stack-cli#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.2"
  }
}
